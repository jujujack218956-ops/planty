{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": true,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "plugins": ["@prettier/plugin-php"],
  "phpVersion": "7.4",
  "overrides": [
    {
      "files": "*.php",
      "options": {
        "tabWidth": 4,
        "useTabs": true,
        "singleQuote": true,
        "printWidth": 120
      }
    }
  ]
}